<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%gray(%d) [%t] %highlight(%-5p) %m%n</pattern>
    </encoder>
  </appender>

  <root level="info">
    <appender-ref ref="consoleAppender" />
  </root>
</configuration>
